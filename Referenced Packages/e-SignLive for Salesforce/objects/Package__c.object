        <fullName>Signer_Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(AND(Sent__c = false, OR(ISPICKVAL(Status__c, &quot;Prepared&quot;), ISPICKVAL(Status__c, &quot;Draft&quot;))), &quot;Not Signed&quot;,
IF(Number_Of_Locked_Signers__c = 1 ,
Action_Required_Signer__r.First_Name__c + &quot; &quot; + Action_Required_Signer__r.Last_Name__c + &quot; is locked.&quot;,
IF(Number_Of_Locked_Signers__c &gt; 1 ,
TEXT(Number_Of_Locked_Signers__c) + &quot; signers are locked.&quot;,
IF((Total_Number_Of_Signers__c - Number_Of_Signed_Signers__c - Number_Of_Signature_Not_Required_Signers__c) = 1,
Action_Required_Signer__r.First_Name__c + &quot; &quot;+ Action_Required_Signer__r.Last_Name__c+&quot; must sign.&quot;,
IF(
AND((Total_Number_Of_Signers__c-Number_Of_Signed_Signers__c-Number_Of_Signature_Not_Required_Signers__c)&gt;1,
Enforce_Signing_Order__c = true),
Action_Required_Signer__r.First_Name__c + &quot; &quot;+ Action_Required_Signer__r.Last_Name__c+&quot; must sign.&quot;,
IF((Total_Number_Of_Signers__c-Number_Of_Signed_Signers__c-Number_Of_Signature_Not_Required_Signers__c)&gt;1,
TEXT(Total_Number_Of_Signers__c-Number_Of_Signed_Signers__c-Number_Of_Signature_Not_Required_Signers__c)+&quot;/&quot;+TEXT(Total_Number_Of_Signers__c)+&quot; signers must sign.&quot;,
IF((Total_Number_Of_Signers__c-Number_Of_Signed_Signers__c-Number_Of_Signature_Not_Required_Signers__c)=0,
&quot;All signers have signed.&quot;,&quot;&quot;
)
)
)
)
)
)
)</formula>
