    <validationRules>
        <fullName>Expiry_Date_Should_Not_Be_In_Past</fullName>
        <active>true</active>
        <description>Prevents user from selecting a previous date as expiry date</description>
        <errorConditionFormula>NOT(ISBLANK( Expiry_Date__c )) &amp;&amp; (Expiry_Date__c &lt;=  TODAY()) &amp;&amp;  NOT(ISPICKVAL(Status__c , &apos;Expired&apos;)) &amp;&amp; (ISBLANK( From_Package_Template__c ) &amp;&amp; Locked__c = false &amp;&amp;  NOT(ISCHANGED(Restore_Pending__c)) &amp;&amp; Restore_Pending__c=false)</errorConditionFormula>
        <errorMessage>You cannot update this package because the Expiry Date is today or in the past, select a future expiry date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Validation</fullName>
        <active>true</active>
        <description>Validate whether the Account and Opportunity is related</description>
        <errorConditionFormula>NOT(ISBLANK(Opportunity__c)) &amp;&amp; NOT(ISBLANK(Account__c)) &amp;&amp; Opportunity__r.AccountId != Account__c</errorConditionFormula>
        <errorDisplayField>Opportunity__c</errorDisplayField>
        <errorMessage>Opportunity and Account are not related.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Package_Status_Validation</fullName>
        <active>true</active>
        <description>The Status of the package cannot be reverted back.</description>
        <errorConditionFormula>ISPICKVAL(Status__c, &apos;Draft&apos;) &amp;&amp; (ISPICKVAL(PRIORVALUE(Status__c), &apos;Sent&apos;) || ISPICKVAL(PRIORVALUE(Status__c), &apos;Completed&apos;))</errorConditionFormula>
        <errorMessage>This package is either Sent or Completed and cannot be moved back to Draft.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_User_First_Name</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK($User.FirstName)</errorConditionFormula>
        <errorMessage>We detected you don&apos;t have a First Name vale in your Salesforce settings. Please Set a First Name to use this feature</errorMessage>
    </validationRules>
