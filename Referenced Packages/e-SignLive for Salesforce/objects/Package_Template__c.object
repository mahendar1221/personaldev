    <validationRules>
        <fullName>Expiry_Date_Should_Not_Be_In_Past</fullName>
        <active>true</active>
        <description>Prevents user from selecting a previous date as expiry date</description>
        <errorConditionFormula>NOT(ISBLANK( Expiry_Date__c )) &amp;&amp; (Expiry_Date__c &lt;= TODAY()) &amp;&amp; NOT(ISPICKVAL(Status__c , &apos;Expired&apos;)) &amp;&amp; NOT(ISPICKVAL(Status__c , &apos;Inactive&apos;))</errorConditionFormula>
        <errorMessage>You cannot update this template because the Expiry Date is today or in the past, select a future expiry date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_User_First_Name</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( $User.FirstName )</errorConditionFormula>
        <errorMessage>We detected you don&apos;t have a First Name vale in your Salesforce settings. Please Set a First Name to use this feature</errorMessage>
    </validationRules>
