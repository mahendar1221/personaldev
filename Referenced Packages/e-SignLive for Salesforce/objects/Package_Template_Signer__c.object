    <validationRules>
        <fullName>Contact_Email_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Type__c , &apos;Contact&apos;) &amp;&amp; ISBLANK(Email__c) &amp;&amp; ISBLANK(Contact__r.Email)</errorConditionFormula>
        <errorDisplayField>Email__c</errorDisplayField>
        <errorMessage>Contact Email is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_First_Name_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Type__c , &apos;Contact&apos;) &amp;&amp; ISBLANK(First_Name__c) &amp;&amp; ISBLANK(Contact__r.FirstName)</errorConditionFormula>
