        <errorDisplayField>First_Name__c</errorDisplayField>
        <errorMessage>First Name is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Last_Name_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Type__c , &apos;External Email&apos;) &amp;&amp; ISBLANK(Last_Name__c)</errorConditionFormula>
