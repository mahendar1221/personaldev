    <validationRules>
        <fullName>Contact_Phone_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Authentication_Method__c , &apos;SMS&apos;) &amp;&amp; ISPICKVAL(Type__c , &apos;Contact&apos;) &amp;&amp; ISBLANK(Phone__c) &amp;&amp; ISBLANK(Contact__r.Phone)</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>Contact Phone is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Required_Values_Validation</fullName>
        <active>false</active>
        <description>This is to test if validations for first names can be combined.</description>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &apos;Contact&apos; ), NOT(ISBLANK(Contact__c)), OR(ISBLANK(First_Name__c) &amp;&amp; ISBLANK(Contact__r.FirstName), ISBLANK(Last_Name__c) &amp;&amp; ISBLANK(Contact__r.LastName),  ISBLANK(Email__c) &amp;&amp; ISBLANK(Contact__r.Email)))</errorConditionFormula>
        <errorMessage>Contact First Name, Last Name And Email are required values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Email_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Type__c , &apos;External Email&apos;) &amp;&amp; ISBLANK(Email__c)</errorConditionFormula>
        <errorDisplayField>Email__c</errorDisplayField>
        <errorMessage>Email is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_First_Name_Validation</fullName>
        <active>false</active>
