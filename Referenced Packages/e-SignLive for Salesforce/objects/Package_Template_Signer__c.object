        <errorMessage>Last Name is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Phone_Validation</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(Authentication_Method__c , &apos;SMS&apos;) &amp;&amp; ISPICKVAL(Type__c , &apos;External Email&apos;) &amp;&amp; ISBLANK(Phone__c)</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>Phone is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Required_Values_Validation</fullName>
        <active>false</active>
        <description>Validates whether first name, last name and email values are entered for the &apos;External Email&apos; type package template signer or not.</description>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &apos;External Email&apos; ), OR(ISBLANK(First_Name__c), ISBLANK(Last_Name__c), ISBLANK(Email__c) &amp;&amp; ISBLANK(Email__c)))</errorConditionFormula>
        <errorMessage>First Name, Last Name and Email are required values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Phone_Validation</fullName>
        <active>false</active>
        <description>Validates whether Phone is entered for &apos;SMS&apos; type authentication for Lead type package template singer.</description>
        <errorConditionFormula>ISPICKVAL(Authentication_Method__c , &apos;SMS&apos;) &amp;&amp; ISPICKVAL(Type__c , &apos;Lead&apos;) &amp;&amp; ISBLANK(Phone__c) &amp;&amp; ISBLANK(Lead__r.Phone)</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>Lead Phone is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Required_Values_Validation</fullName>
        <active>false</active>
        <description>Validates whether first name, last name or Email has been entered for contact type signer or not.</description>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &apos;Lead&apos; ), NOT(ISBLANK(Lead__c)),OR(ISBLANK(First_Name__c) &amp;&amp; ISBLANK(Lead__r.FirstName), ISBLANK(Last_Name__c) &amp;&amp; ISBLANK(Lead__r.LastName), ISBLANK(Email__c) &amp;&amp; ISBLANK(Lead__r.Email)))</errorConditionFormula>
        <errorMessage>Lead First Name, Last Name And Email are required values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Format</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT( ISBLANK(Phone__c)), NOT(ISPICKVAL(Type__c , &apos;&apos;)), NOT(REGEX(Phone__c, &quot;(1\\s*[-\\/\\.]?)?(\\((\\d{3})\\)|(\\d{3}))\\s*[-\\/\\.]?\\s*(\\d{3})\\s*[-\\/\\.]?\\s*(\\d{4})\\s*(([xX]|[eE][xX][tT])\\.?\\s*(\\d+))*&quot;)))</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>You must enter a valid phone number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_Phone_Validation</fullName>
        <active>false</active>
        <description>Validates whether phone value is entered for &apos;SMS&apos; authentication type or not.</description>
        <errorConditionFormula>ISPICKVAL(Authentication_Method__c , &apos;SMS&apos;) &amp;&amp; ISPICKVAL(Type__c , &apos;User&apos;) &amp;&amp; ISBLANK(Phone__c) &amp;&amp; ISBLANK(User__r.Phone)</errorConditionFormula>
        <errorDisplayField>Phone__c</errorDisplayField>
        <errorMessage>User Phone is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>User_Required_Values_Validation</fullName>
        <active>false</active>
        <description>Validates whether first name, last name or Email has been entered for contact type signer or not.</description>
        <errorConditionFormula>AND(ISPICKVAL(Type__c, &apos;User&apos; ), NOT(ISBLANK(User__c)),OR(ISBLANK(First_Name__c) &amp;&amp; ISBLANK(User__r.FirstName), ISBLANK(Last_Name__c) &amp;&amp; ISBLANK(User__r.LastName), ISBLANK(Email__c) &amp;&amp; ISBLANK(User__r.Email)))</errorConditionFormula>
        <errorMessage>User First Name, Last Name And Email are required values.</errorMessage>
    </validationRules>
